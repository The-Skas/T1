func interact():
	print(\"Im a real entity!\")


	
	if(toggle_radio):
		$AudioStreamPlayer2D.play()
		$Particle_Music.emitting = true
		toggle_radio = false
		get_node(\"Event\").this_happened(\"radio_on\")

	else:
		$AudioStreamPlayer2D.stop()
		$Particle_Music.emitting = false
		toggle_radio = true
