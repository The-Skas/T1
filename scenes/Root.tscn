var fixed_pos
var time = 0
var alarm_time = null
onready var pot_smoke = get_node(\"../../../Top/pot_smoke\")
func _ready():
	# Called when the node is added to the scene for the first time.
	# Initialization here
	pass
	set_process(true)
	fixed_pos = self.position

func _process(delta):
	if(alarm_time != null):
		if(Globals.timer.time_left < alarm_time):
	#		get_node(\"Event\").this_happened(\"smoke\")
			if( not $Sound_Alarm.playing):
				$Sound_Alarm.play()
				$Sound_Tick.stop()
				$Event.this_happened(\"alarm\")
			self.position.x = fixed_pos.x + sin(Globals.timer.time_left*100) * 0.8

		

		
		

func rewind():
	self.position = fixed_pos
	self.rotation = 0
	time = 0

#func _process(delta):
#	# Called every frame. Delta is time since last frame.
#	# Update game logic here.
#	pass
"

